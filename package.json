    "tailwindcss": "^4.1.10",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3",
    "typescript-eslint": "^7.0.1",
    "vite": "^5.1.3",
    "vitest": "^3.2.3"
